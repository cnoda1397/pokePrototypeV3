{
  "database": {
    "rules": "database.rules.json"
  },
  "hosting": [
    {
      "target": "app",
      "public": "public",
      "headers": [
        {
          "source": "**/*.@(eot|otf|ttf|ttc|woff|font.css)",
          "headers": [
            {
              "key": "Access-Control-Allow-Origin",
              "value": "*"
            }
          ]
        }
      ]
    },
    {
      "target": "dex",
      "public": "dex"
    },
    {
      "target": "dex-reset",
      "public": "reset"
    }
  ],
  "storage": {
    "rules": "storage.rules"
  },
  "emulators": {
    "database": {
      "port": 9000
    },
    "hosting": {
      "port": 5000
    },
    "ui": {
      "enabled": true
    }
  },
  "functions": {

  }
}
